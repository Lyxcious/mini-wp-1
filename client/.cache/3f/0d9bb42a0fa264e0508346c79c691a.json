iner home\" v-show=\"page == 'home'\">\n    <div class=\"row\" style=\"height: 100%\">\n      <div class=\"col-2 p-0\" style=\"border-right-width: 3px; border-right-color: rgba(202, 202, 202, 0.596); border-right-style: solid\">\n        <h3 class=\"pl-3 pr-3\">Menu bar</h3>\n        <div class=\"pl-3 pr-3\">\n        <hr/>\n        </div>\n        <div id=\"menu-bar\">\n          <div class=\"menu pl-3 pr-3 pt-2 pb-2\" @click=\"rendAdd\" v-bind:class=\"{'menu-active':(menu == 'add')}\">\n            <a href=\"#\" class=\"menu\"><i class=\"fas fa-feather-alt\"></i> Add Article</a>\n          </div>\n          <div class=\"menu pl-3 pr-3 pt-2 pb-2\" @click=\"rendList\" v-bind:class=\"{'menu-active':(menu == 'list')}\">\n            <a href=\"#\" class=\"menu\"><i class=\"fas fa-bars\"></i> List Article</a>\n          </div>\n          <div class=\"menu pl-3 pr-3 pt-2 pb-2 disabled\" v-bind:class=\"{'menu-active':(menu == 'edit')}\">\n            <a href=\"#\" class=\"menu\"><i class=\"fas fa-edit\"></i> Edit Article</a>\n          </div>\n          <div class=\"menu pl-3 pr-3 pt-2 pb-2 disabled\" v-bind:class=\"{'menu-active':(menu == 'detail')}\">\n            <a href=\"#\" class=\"menu\"><i class=\"fas fa-info-circle\"></i> Article Detail</a>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"col\" id=\"content\">\n        <div v-show=\"menu == 'add'\">\n          <div class=\"center d-flex justify-content-center\">\n            <h2>Article Form</h2>\n          </div>\n          <div class=\"center\" style=\"width: 50%\">\n            <hr>\n          </div>\n          <form class=\"form-article center\" @submit.prevent=\"addArticle\" enctype=\"multipart/form-data\">\n              <div class=\"form-group\">\n                <label>Title</label>\n                <input type=\"text\" class=\"form-control\" v-model=\"article.title\" placeholder=\"Enter Article Title\" required>\n              </div>\n              <div class=\"form-group d-flex\">\n                <div class=\"row\">\n                  <div class=\"col\">\n                    <img v-bind:src=\"article.img\" width=\"100\" height=\"100\" class=\"mr-1\">\n                  </div>\n                </div>\n                <div class=\"row d-flex flex-column\">\n                  <div class=\"col\">\n                    <label>Image</label>\n                  </div>\n                  <div class=\"col\">\n                    <input type=\"file\" class=\"border\" style=\"border-radius: 5px\" accept=\"image/png, image/jpeg\" @change=\"changeImg($event)\">\n                  </div>\n                </div>\n              </div>\n              <div class=\"form-group\">\n                <label>Content</label>\n                <wysiwyg v-model=\"article.content\"></wysiwyg>\n              </div>\n              <div class=\"col text-center\">\n                <button type=\"submit\" class=\"btn btn-primary center\">Submit</button>\n              </div>\n          </form>\n        </div>\n\n        <div v-show=\"menu == 'edit'\">\n          <div class=\"center d-flex justify-content-center\">\n            <h2>Edit Article Form</h2>\n          </div>\n          <div class=\"center\" style=\"width: 50%\">\n            <hr>\n          </div>\n          <form class=\"form-article center\" @submit.prevent=\"updateArticle\" enctype=\"multipart/form-data\">\n              <div class=\"form-group\">\n                <label >Title</label>\n                <input type=\"text\" class=\"form-control\" v-model=\"article.title\" placeholder=\"Enter Article Title\" required>\n              </div>\n              <div class=\"form-group d-flex\">\n                  <div class=\"row\">\n                    <div class=\"col\">\n                      <img v-bind:src=\"article.img\" width=\"100\" height=\"100\" class=\"mr-1\">\n                    </div>\n                  </div>\n                  <div class=\"row d-flex flex-column\">\n                    <div class=\"col\">\n                      <label>Image</label>\n                    </div>\n                    <div class=\"col\">\n                      <input type=\"file\" class=\"border\" style=\"border-radius: 5px\" accept=\"image/png, image/jpeg\" @change=\"changeImg($event)\">\n                    </div>\n                  </div>\n                </div>\n              <div class=\"form-group\">\n                <label >Content</label>\n                <wysiwyg v-model=\"article.content\"></wysiwyg>\n              </div>\n              <div class=\"col text-center\">\n                <button type=\"submit\" class=\"btn btn-primary center\">Submit</button>\n              </div>\n          </form>\n        </div>\n\n        <div v-show=\"menu == 'list'\" style=\"height: 700px\">\n            <div class=\"center d-flex flex-column justify-content-center\">\n              <h2 class=\"center\">List Article</h2>\n              <form id=\"filter\" class='center align-middle' @submit.prevent=\"getFilter\">\n                  <div class=\"row\">\n                      <div class=\"col\">\n                        <div class=\"input-group input-group-md\">\n                          <input type=\"text\" name=\"filter\" v-model=\"filter\" class=\"form-control form-control-md input-md\" placeholder=\"Filter By Article Title\">\n                          <span class=\"input-group-append\">\n                            <button class=\"btn btn-success btn-md\" type=\"submit\">Search</button>\n                          </span>\n                        </div>\n                      </div>\n                  </div>\n                  <div class=\"row\"  style=\"margin-top: 15px\">\n                    <div class=\"col\">\n                      <div class=\"input-group input-group-md d-flex justify-content-center\">\n                        <button class=\"btn btn-success btn-md\" type=\"button\" @click=\"getAll\">Get All Article</button>\n                      </div>\n                    </div>\n                </div>\n                </form>\n            </div>\n            <div class=\"center\" style=\"width: 90%\">\n              <hr>\n            </div>\n            <div class=\"center align-items-center d-flex flex-column\" id=\"article-list\" style=\"overflow: scroll; height: 500px\">\n              <div class=\"card mb-3 article\" style=\"width: 70vw;\" :key=\"article.id\" v-for=\"article in articles\">\n                  <div class=\"row no-gutters\">\n                    <div class=\"col-2\" style=\"background-size: cover; background-repeat: no-repeat; background-position: 50% 50%;\" v-bind:style=\"{ backgroundImage: 'url(' + article.img + ')' }\">\n                    </div>\n                    <div class=\"col-10\">\n                      <div class=\"card-body\">\n                        <h5 class=\"card-title\">{{article.title}}</h5>\n                        <div class=\"row d-flex justify-content-start\" style=\"height: 2em; overflow: hidden;\">\n                          <div class=\"col pr-0\" style=\"overflow: hidden\">\n                            <div class=\"card-text\" v-html=\"article.content\"></div>\n                          </div>\n                        </div>\n                        <p class=\"card-text\">Created at: {{convertDate(article.created_at)}}</p>\n                        <a href=\"#\" @click=\"getDetail(article._id)\">See Detail</a>\n                      </div>\n                    </div>\n                  </div>\n              </div>\n            </div>\n            <div class=\"d-flex justify-content-center\">\n              <div style=\"width: 42.5%\">\n                <hr>\n              </div>\n              <div style=\"width: 5%\" class=\"d-flex justify-content-center\">\n                <img src=\"https://s.w.org/style/images/about/WordPress-logotype-wmark.png\" width=\"40\" height=\"40\" alt=\"logo\">\n              </div>\n              <div  style=\"width: 42.5%\">\n                <hr>\n              </div>\n            </div>\n        </div>\n\n        <div v-show=\"menu == 'detail'\" style=\"height: 700px\">\n          <div class=\"center align-items-center d-flex flex-column\" id=\"article-list\" style=\"height: 100%\">\n            <div class=\"card article\" style=\"width: 70vw; height: 100%\">\n              <div class=\"row no-gutters d-flex flex-column justify-content-between\" style=\"height: 100%\">\n                <div class=\"card-body d-flex flex-column justify-content-between\" >\n                  <div class=\"row d-flex flex-column p-3\" >\n                    <h5 class=\"card-title center\">{{article.title}}</h5>\n                    <div class=\"center\" style=\"width: 100%\">\n                      <hr>\n                    </div>\n                    <div class=\"d-flex flex-column justify-content-between\" style=\"height: 500px\">\n                      <div class=\"row pl-3\" style=\"height: 450px; width: 100%\">\n                        <div class=\"col-3\" style=\"background-size: cover; background-repeat: no-repeat; background-position: 50% 50%;\" v-bind:style=\"{ backgroundImage: 'url(' + article.img + ')' }\"></div>\n                        <div class=\"col\" style=\"overflow: scroll\">\n                          <div class=\"card-text\" v-html=\"article.content\"></div>\n                        </div>\n                      </div>\n                      <p class=\"card-text\">Created at: {{convertDate(article.created_at)}}</p>\n                    </div>\n                  </div>\n                  <div class=\"row d-flex justify-content-end\">\n                    <div class=\"center pr-3 pl-3\" style=\"width: 100%\">\n                      <hr>\n                    </div>\n                    <div class=\"pr-3\">\n                      <button class=\"btn btn-success\" @click=\"rendEdit\">Edit</button>\n                      <button class=\"btn btn-danger\" @click=\"deleteArticle\">Delete</button>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    'page' : String,\n    'menu' : String,\n    'article' : Object,\n    'filter' : String,\n    'articles' : Array,\n  },\n  data() {\n    return {\n    };\n  },\n  methods: {\n    rendAdd () {\n      this.$emit('rendAdd')\n    },\n    rendList () {\n      this.$emit('rendList')\n    },\n    addArticle () {\n      this.$emit('addArticle')\n    },\n    changeImg ($event) {\n      this.$emit('changeImg', $event)\n    },\n    updateArticle () {\n      this.$emit('updateArticle')\n    },\n    getFilter () {\n      this.$emit('getFilter')\n    },\n    getAll () {\n      this.$emit('getAll')\n    },\n    convertDate (date) {\n      let createdDate = new Date (date)\n      var days = [\"Sunday\", \"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\"];\n      return `${days[createdDate.getDay()]}, ${createdDate.toString().slice(4,25)}`\n    },\n    getDetail (id) {\n      this.$emit('getDetail', id)\n    },\n    rendEdit () {\n      this.$emit('rendEdit')\n    },\n    deleteArticle () {\n      this.$emit('deleteArticle')\n    }\n  }\n};\n</script>\n\n<style scoped>\n  .container{\n    max-width: 100%;\n    margin: 15px 0;\n    height: 700px;\n  }\n  .form-article {\n    width: 50%;\n  }\n  #filter {\n    width: 50%;\n  }\n  .center {\n    margin: 0px auto;\n  }\n  .menu a {\n    font-size: 20px;\n    color: black;\n  }\n  .menu-active {\n    background-color: rgba(202, 202, 202, 0.596);\n  }\n  .menu-active a {\n    color: #006bc3;\n  }\n  .menu:hover > a {\n    text-decoration: none;\n    opacity: 0.6;\n  }\n  .menu:hover {\n    cursor: pointer;\n  }\n  .disabled:hover > a {\n    cursor: default;\n    opacity: 1;\n  }\n  .disabled a:hover {\n    text-decoration: none;\n    opacity: 1;\n    cursor: default;\n  }\n  .disabled:hover {\n    cursor: default;\n  }\n  hr {\n    border-width: 3px;\n    border-color: rgb(73, 73, 73); \n  }\n</style>\n"},"lineCount":null}},"error":null,"hash":"a477cae33ba90e507bc8134c93b8e50f","cacheData":{"env":{}}}